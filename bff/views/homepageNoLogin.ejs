<div class="layout">
        <div class="header">
            <div class="header-left">
                <span class="header-left_logo">
                    <img src="OIP.jpg" alt="" style="color: #fff">
                </span>
                <span class="header-left_icon-homepage">
                    <i class="fa-solid fa-house"></i>
                </span>
                <div class="header-left_search">
                    <!-- Hoi chat gpt xem cái khung search này template như nào-->
                    <i class="fa-solid fa-magnifying-glass"></i>
                    <input type="text" placeholder="Bạn muốn tìm nội dung gì?">
                    <i class="fa-solid fa-folder-open"></i>
                </div>
                <div class="header-left_search--history">
                    <div class="header-left_search--history-header">Các tìm kiếm gần đây</div>
                    <div class="header-left_search--history-item">
                        <img src="ST.jpg" alt="st-MTP" class="header-left_search--history-icon">
                        <div class="header-left_search--history-texts">
                            <div class="header-left_search--history-title">Cà phê sữa</div>
                            <div class="header-left_search--history-subtitle">Thức uống</div>
                        </div>
                    </div>
                    <div class="header-left_search--history-item">
                        <img src="ST.jpg" alt="st-MTP" class="header-left_search--history-icon">
                        <div class="header-left_search--history-texts">
                            <div class="header-left_search--history-title">Cà phê sữa</div>
                            <div class="header-left_search--history-subtitle">Thức uống</div>
                        </div>
                    </div>
                    <button class="header-left_search--history-clear-button">Xoá các tìm kiếm gần đây</button>
                </div>
            </div>
            <div class="header-right">
                <div class="header-right_menu">
                    <ul class="header-right_menu--list">
                        <li class="header-right_menu--item">Thông tin</li>
                        <li class="header-right_menu--item">Liên hệ</li>
                        <li class="header-right_menu--item">Tải xuống</li>
                    </ul>
                    <div class="header-right_menu_download">
                        <i class="fa-solid fa-right-to-bracket"></i>
                        <span class="header-right_menu_download--text">Đăng nhập cho Admin</span>
                    </div>
                </div>
                <div class="header-right_login">
                    <span class="header-right_login--signUp">Đăng ký</span>
                    <span class="header-right_login--signIn">Đăng nhập</span>
                </div>
            </div>
        </div>
        <div class="container">
            <!-- size 1/4-->
            <div class="container-sidebar">

                <div class="container-sidebar_box4">
                    <ul class="container-sidebar_box4-playlist">
                        <li class="container-sidebar_box4-playlist--item selected">
                            <div class="container-sidebar_box4-playlist-info">
                                <div class="container-sidebar_box4-playlist-title">Homepage</div>
                            </div>
                        </li>
                        <li class="container-sidebar_box4-playlist--item">
                            <div class="container-sidebar_box4-playlist-info">
                                <div class="container-sidebar_box4-playlist-title">Product</div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
            <!-- size 3/4 -->
            <div class="container-box-content">
                <!-- Những bài hát thịnh hành -->
                <div class="container-box-component">
                    <div class="container-box-component--header">
                        <div class="container-box-component--header--div">
                            <span class="container-box-component--header--text">Những sản phẩm thịnh hành</span>
                            <span class="container-box-component--header--showAll">Hiện tất cả</span>
                        </div>
                        <div class="slider-container">
                            <button class="container-box-component--header--list--btn-left">&#10094;</button>
                            <ul class="container-box-component--header--list">

                                <li class="container-box-component--header--item">
                                    <div class="song-thumbnail">
                                        <img src="imgSong.jpg" alt=""
                                            class="container-box-component--header--item--img" />
                                        <span class="container-box-component--header--item--icon-play">
                                            <i class="fa-solid fa-cart-shopping"></i>
                                        </span>
                                    </div>
                                    <span class="container-box-component--header--item--nameSong">Cà phê</span>
                                    <span class="container-box-component--header--item--artist">Cost: 10$</span>
                                </li>


                            </ul>
                            <button class="container-box-component--header--list--btn-right">&#10095;</button>
                        </div>
                    </div>
                </div>
                <!-- Những nghệ sĩ nổi tiếng  -->
